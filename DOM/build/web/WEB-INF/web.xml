<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login_Controller</servlet-name>
        <servlet-class>com.domdemo.servlet.dom.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>com.domdemo.servlet.dom.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddController</servlet-name>
        <servlet-class>com.domdemo.servlet.dom.AddController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateController</servlet-name>
        <servlet-class>com.domdemo.servlet.dom.UpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_stax</servlet-name>
        <servlet-class>com.domdemo.servlet.stax.Login_stax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_stax</servlet-name>
        <servlet-class>com.domdemo.servlet.stax.Add_stax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_stax</servlet-name>
        <servlet-class>com.domdemo.servlet.stax.Delete_stax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_stax</servlet-name>
        <servlet-class>com.domdemo.servlet.stax.Update_stax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadServlet</servlet-name>
        <servlet-class>com.domdemo.servlet.dom.LoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login_Controller</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddController</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateController</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_stax</servlet-name>
        <url-pattern>/login_stax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_stax</servlet-name>
        <url-pattern>/delete_stax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_stax</servlet-name>
        <url-pattern>/add_stax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_stax</servlet-name>
        <url-pattern>/update_stax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadServlet</servlet-name>
        <url-pattern>/load</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
